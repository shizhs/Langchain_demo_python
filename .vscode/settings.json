{
  "files.exclude": {
    "out": true,
    "dist": true,
    "**/*.pyc": true,
    ".nyc_output": true,
    "obj": true,
    "bin": true,
    "**/__pycache__": true,
    "**/node_modules": true,
    ".vscode-test": false,
    ".vscode test": false,
    "**/.mypy_cache/**": true,
    ".venv": true,
    ".ipynb_checkpoints": true,
    ".pytest_cache": true,
    "scripts": true,
  },
  "search.exclude": {
    "out": true, // set this to false to include "out" folder in search results
    "dist": true,
    "**/node_modules": true,
    "coverage": true,
    "languageServer*/**": true,
    ".vscode-test": true,
    ".vscode test": true,
    "**/__pycache__": true
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff"
